{
  "listeners": {
    "*:80": {
      "pass": "applications/express"
    }
  },

  "applications": {
    "express": {
      "processes": 9,
      "type": "external",
      "working_directory": "/app",
      "executable": "/usr/bin/env",
      "arguments": [
        "node",
        "--loader",
        "unit-http/loader.mjs",
        "--require",
        "unit-http/loader",
        "./main.js"
      ]
    }
  }
}
